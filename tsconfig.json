{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "baseUrl": "./",
    "paths": {
      "@Components/*": ["src/components/*"],
      "@Pages/*": ["src/pages/*"],
      "@Types/*": ["src/types/*"],
      "@Store/*": ["src/store/*"],
      "@Api/*": ["src/api/*"],
    }
  },
  "include": ["src"]
}
